<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/aparelhos_main_frag"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AparelhosMenu">

    <!-- TODO: Update blank fragment layout -->

    <fragment
        android:id="@+id/aparelho_list_frag"
        android:name="com.example.laura.economia.AparelhosList"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout="@layout/fragment_aparelhoslist_list" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/novo_user_btn"
        android:layout_width="67dp"
        android:layout_height="70dp"
        android:layout_marginBottom="32dp"
        android:layout_marginEnd="28dp"
        android:layout_marginRight="28dp"
        android:clickable="true"
        android:onClick="AbrirAdicionarAparelho"
        app:backgroundTint="@color/verde2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/ic_plug_add_icon" />
</android.support.constraint.ConstraintLayout>